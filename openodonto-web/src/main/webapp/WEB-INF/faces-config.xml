<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <!-- Componentes -->
 <component>
  <component-type>mapComponent</component-type>
  <component-class>br.ueg.openodonto.web.tags.HtmlMap</component-class>
 </component>
 <!-- Conversores -->
 <converter>
  <converter-id>SqlDateConverter</converter-id>
  <converter-class>br.ueg.openodonto.visao.converter.SqlDateConverter</converter-class>
 </converter>
 <converter>
  <converter-id>simpleEntityConverter</converter-id>
  <converter-class>br.ueg.openodonto.visao.converter.SimpleIndexConverter</converter-class>
 </converter>
 <converter>
  <converter-id>questaoConverter</converter-id>
  <converter-class>br.ueg.openodonto.visao.converter.QuestaoQuestionarioConverter</converter-class>
 </converter>
 <!-- ManagedBeans -->
 <managed-bean>
  <managed-bean-name>manterPaciente</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterPaciente</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterDentista</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterDentista</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterUsuario</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterUsuario</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterColaborador</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterColaborador</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterProduto</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterProduto</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterListagem</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.servico.ManageListagem</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>login</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.Login</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterProcedimento</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterProcedimento</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterQuestaoAnamnese</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterQuestaoAnamnese</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manterQuestionarioAnamnese</managed-bean-name>
  <managed-bean-class>br.ueg.openodonto.controle.ManterQuestionarioAnamnese</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <!-- Regras de navegação -->
 <navigation-rule>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>managePaciente</from-outcome>
   <to-view-id>/cadastro/paciente/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageDentista</from-outcome>
   <to-view-id>/cadastro/dentista/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageProduto</from-outcome>
   <to-view-id>/cadastro/produto/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageFornecedor</from-outcome>
   <to-view-id>/cadastro/colaborador/fornecedor/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>managePrestador</from-outcome>
   <to-view-id>/cadastro/colaborador/prestador/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageUsuario</from-outcome>
   <to-view-id>/cadastro/usuario/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageProcedimento</from-outcome>
   <to-view-id>/cadastro/procedimento/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>manageQuestao</from-outcome>
   <to-view-id>/cadastro/anamnese/questao/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>manageQuestionario</from-outcome>
   <to-view-id>/cadastro/anamnese/questionario/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>
